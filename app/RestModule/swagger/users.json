{
    "basePath": "BASE_PATH",
    "swaggerVersion": "1.2",
    "apiVersion": "1.0.0",
    "resourcePath": "/users",
    "apis": [
        {
            "path": "/users",
            "description": "Akce pro vytvo\u0159en\u00ed nov\u00e9ho u\u017eivatelsk\u00e9ho \u00fa\u010dtu na z\u00e1klad\u011b zaslan\u00fdch hodnot",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Create new user account",
                    "nickname": "actionCreate",
                    "type": "UserResponse",
                    "parameters": [
                        {
                            "paramType": "body",
                            "type": "UserInput",
                            "required": true,
                            "description": "User"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 201,
                            "message": "User account created successfully, returns details of User."
                        },
                        {
                            "code": 404,
                            "message": "Requested user was not found."
                        }
                    ]
                }
            ]
        },
        {
            "path": "/users/{id}",
            "description": "Akce vracej\u00edc\u00ed detaily konkr\u00e9tn\u00edho u\u017eivatelsk\u00e9ho \u00fa\u010dtu",
            "operations": [
                {
                    "method": "GET",
