					<MaxOccurrences>1</MaxOccurrences>
					<Field hidden="true">
						<Name>minLength</Name>
						<DefaultValue>1</DefaultValue>
						<LocalizedName lang="en">Minimum number of values</LocalizedName>
						<LocalizedName lang="cs">Minimální délka</LocalizedName>
						<Validation>
